	<div class="loader">
		<div class="loader-main"></div>
	</div>
    
	<% if(success != '') { %>
			<div class="master-success-message alert alert-success"> 
				<%- success %> 
			</div>
	<% } %>
	
	<%- include('navbar/navbar') %>

	<%- content %>

	<!--Libraries-->
	<script src="./libraries/bower_components/jquery/dist/jquery.min.js"></script>
	<script src="./libraries/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.1/socket.io.js"></script>
	<script src="./libraries/bower_components/jquery.nicescroll/jquery.nicescroll.min.js"></script>
	<script src="./libraries/bower_components/AlertifyJS/build/alertify.js"></script>
	<script src="./libraries/bower_components/moment/min/moment.min.js"></script>
	<script src="./libraries/bower_components/moment/min/moment-with-locales.min.js"></script>
	<script src="./libraries/bower_components/emojionearea/dist/emojionearea.min.js"></script>
	<script src="./libraries/emojione/lib/js/emojione.min.js"></script>
	<script src="./libraries/bower_components/peerjs/peer.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
	<!-- <script src="./libraries/bower_components/sweetalert2/dist/sweetalert2.all.min.js"></script> -->
	<script src="./libraries/bower_components/photoset-grid/jquery.photoset-grid.min.js"></script>
	<script src="./libraries/bower_components/jquery-colorbox/jquery.colorbox-min.js"></script>
	<script src="socket.io/socket.io.js"></script>

	<script src="./libraries/js/mainConfig.js"></script>
	<script src="./libraries/js/updateProfile.js"></script>

	<script src="./libraries/js/calculate.js"></script>
	<script src="./libraries/js/requestContact.js"></script>
	<script src="./libraries/js/findUsers.js"></script>
	<script src="./libraries/js/acceptRequestContactReceived.js"></script>
	<script src="./libraries/js/removeContact.js"></script>

	<script src="./libraries/js/readMoreNotif.js"></script>
	<script src="./libraries/js/markAllAsRead.js"></script>

	<script src="./libraries/js/readMoreContacts.js"></script>
	<script src="./libraries/js/readMoreContactsSent.js"></script>
	<script src="./libraries/js/readMoreContactsReceived.js"></script>

	<script src="./libraries/js/chatImage.js"></script>
	<script src="./libraries/js/chatText.js"></script>
	<script src="./libraries/js/typingMessage.js"></script>

	<%- include('contact/contactModal') %>

	<%- include('stream/streamModal') %>

	<%- include('image/imageModal') %>

	<%- include('attach/attachModal') %>

	<%- include('profile/profileModal') %>

	<%- include('groupChat/groupChatModal') %>

	<%- include('notification/notificationModal') %>

